   isbn = {978-92-5-132974-0},
   journal = {The State of the World’s Forests 2022},
   month = {5},
   publisher = {FAO},
   title = {Global Forest Resources Assessment 2020},
   url = {http://www.fao.org/documents/card/en/c/ca9825en},
   year = {2020}
