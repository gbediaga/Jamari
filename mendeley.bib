@article{deLima2019,
   abstract = {Accurate estimates of lumber volume become an important indicator of production and monetary value for a sawmill. However, such estimates are only obtained directly after logging, and no accurate predictions are made for the Amazonian commercial species. In this sense, the objective was to generate equations of lumber volume for commercial species in Amapá (a state in northern Brazil) by adjusting and selecting regression models. The data of 50 logs processed from 10 commercial species were collected, as well as the quantity and volume of sawn products. Sixteen (16) statistical models were adjusted and statistical weights were performed to evaluate the quality of the estimates and to select the best equation by species. In summary, precise estimates of lumber volume can be obtained by the nº16 model for Carapa guianensis, while the models nº13 and nº15 are the most recommended for Dinizia excelsa and Hymenolobium petraeum, respectively. Model 7 presented the best adjustments for Hymenaea courbaril and Vochysia guianensis. Equations using only the log diameter variable suggest less precise estimates. Also, the log volume should be considered as an important predictor variable to obtain the serrated/lumber volume for the different Amazonian commercial species.},
   author = {Robson B. de Lima and Rinaldo L.Caraciolo Ferreira and José A.Aleixo da Silva and Marcelino C. Guedes and Cinthia P. de Oliveira and Diego A.Silva da Silva and Renan M. Santos and Erik Patrik F. Carvalho and Robson Matheus de A. Silva},
   doi = {10.1080/10549811.2018.1546596},
   issn = {1540756X},
   issue = {3},
   journal = {Journal of Sustainable Forestry},
   keywords = {Amazon rainforest,forest production,regression analysis,sawmill},
   month = {4},
   pages = {262-274},
   publisher = {Taylor and Francis Inc.},
   title = {Lumber volume modeling of Amazon Brazilian species},
   volume = {38},
   year = {2019}
}
