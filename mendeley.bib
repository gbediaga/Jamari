@article{Rico-Straffon2023,
   abstract = {Concessions that grant logging rights to firms support economic development based on forest resources. Eco-certifications put sustainability restrictions on the operations of those concessions. For spatially detailed data, including many pre-treatment years, we use new difference-in-differences estimators to estimate 2002–2018 impacts upon Peruvian Amazon forests from both logging concessions and their eco-certifications. We find that the concessions − which in theory could raise or reduce forest loss − did not raise loss, if anything reducing it slightly by warding off spikes in deforestation pressure. Eco-certifications could reduce or raise forest loss, yet we find no significant impacts.},
   author = {Jimena Rico-Straffon and Zhenhua Wang and Stephanie Panlasigui and Colby J. Loucks and Jennifer Swenson and Alexander Pfaff},
   doi = {10.1016/j.jeem.2022.102780},
   issn = {10960449},
   journal = {Journal of Environmental Economics and Management},
   keywords = {Concession,Deforestation,Difference-in-differences,Impact evaluation,Peru,certification},
   month = {3},
   publisher = {Academic Press Inc.},
   title = {Forest concessions and eco-certifications in the Peruvian Amazon: Deforestation impacts of logging rights and logging restrictions},
   volume = {118},
   year = {2023}
}
@article{Chervier2024,
   abstract = {The Congo Basin is home to the second largest tropical humid forest in the world and an important participant in climate change mitigation. While the Democratic Republic of Congo (DRC) boasts the largest proportion of Congo basin forests, it is also the main contributor to forest loss. Industrial logging concession is a forest management model that is likely to expand in the DRC in the coming years as the Government recently announced the probable end of the 2002 moratorium on logging concessions. It is thus crucial to understand whether this model can further the DRC's deforestation reduction efforts. The literature suggests that the effect of titling forestland on avoided deforestation and forest degradation is generally limited, unless environmental restrictions are enforced. The DRC's 2002 forest code introduced a new logging concession model that includes requirements to submit and implement Forest Management Plans (FMP) and increase benefit-sharing with communities. Our paper evaluates the impact of this new model of concession contracts signed in 2011 and 2014 on deforestation and forest degradation. We use remotely sensed data series to estimate difference-in-difference models. We ensured baseline parallel trends in deforestation and similarity in baseline characteristics between control and treated units. We find no evidence that deforestation and forest degradation is significantly different between logging concessions and control areas, even for concessions that had their FMP accepted. Our study highlights the need for evaluating the impact of FMPs once they are fully implemented, the importance to improve degradation outcome variables and to conduct complementary studies analyzing impact pathways.},
   author = {Colas Chervier and Arimatéa C. Ximenes and Blaise Pascal Ntirumenyerwa Mihigo and Charles Doumenge},
   doi = {10.1016/j.worlddev.2023.106393},
   issn = {18735991},
   journal = {World Development},
   keywords = {Deforestation,Democratic Republic of Congo,Forest degradation,Impact evaluation,Logging concessions},
   month = {1},
   publisher = {Elsevier Ltd},
   title = {Impact of industrial logging concession on deforestation and forest degradation in the DRC},
   volume = {173},
   year = {2024}
}
@techReport{Chan2017,
   abstract = {Southeast Asian forests are characterised by the prevalence of trees from the Dipterocarpaceae family. Timbers from the Dipterocarps dominate the timber industry as it produces long, straight and knot free logs which are ideal for plywood and sawntimber. The denser stocking of the Southeast Asia forests also make them targets for unscrupulous developers of agricultural plantations, mines, roads and hydropower. This paper looks at recent development of forest concessions in Lao PDR, Cambodia, Myanmar and Indonesia, with additional information on Papua New Guinea and Malaysia. Huge expanses of forests were badly degraded, and are still being degraded, by extensive bad logging techniques, agricultural conversion, and other economic activities. Conflicts with local communities are common over rights to land. Overall, there is a significant lack of will in governing even though governance systems might be in place but there are pockets of positive developments in land use policy, forest certification and skills upgrading. Concessions forestières de l'Asie du sud-est: petits pas B. CHAN Les forêts de l'Asie du sud-est sont caractérisées par une prédominance des arbres de la famille Dipterocarpaceae. Les bois du Dipterocarps dominent l'industrie du bois car il produisent des bûches longues, droites et sans noeuds, idéales pour le contreplaqué et le bois de coupe. La forte densité des forêts de l'Asie du sud-est en fait également des cibles pour les promoteurs sans scrupules de plantations agricoles, de mines, de routes et d'énergie hydro-électrique. Ce papier examine les développements récents des concessions forestières au Laos, au Cambodge, au Myanmar et en Indonésie, avec des informations supplémentaires sur la Nouvelle-Guinée Papouasie et la Malaisie. D'immenses étendues forestières ont été très dégradées et continuent à l'être par les mauvaises techniques répandues de coupe de bois, de conversion agricole et d'autres activités économiques. Les conflits de droit à la terre avec les communautés locales sont monnaie courante. Un manque de motivation dans la gestion est prévalent, malgré le fait que des systèmes de gestion soient en place. Des poches de développements positifs sont néanmoins perceptibles dans la politique d'utilisation des terres, la certification forestière et l'amélioration des capabilités. Concesiones forestales del sudeste asiático: pequeños avances B. CHAN Los bosques del sudeste asiático se caracterizan por el predominio de árboles de la familia Dipterocarpaceae. Las maderas de las dipterocarpá-ceas dominan la industria de la madera, ya que ofrecen fustes largos, rectos y libres de nudos que son ideales para el contrachapado y la madera de aserrío. Las densas existencias de los bosques del sudeste de Asia las convierten asimismo en el objetivo de inversores sin escrúpulos en plantaciones agrícolas, minas, carreteras y energía hidroeléctrica. Este artículo examina el desarrollo reciente de las concesiones forestales en Laos, Camboya, Myanmar e Indonesia, así como información adicional sobre Papúa Nueva Guinea y Malasia. Hasta la fecha se han degra-dado enormes extensiones de bosque, y aun se están degradando, debido a malas técnicas de tala, la conversión agrícola y otras actividades económicas. Los conflictos con las comunidades locales sobre los derechos a la tierra son comunes. En general, existe una importante falta de voluntad en cuanto a gobernanza, incluso aunque existan dichos sistemas de gobernanza, pero existen focos de casos positivos relacionados con la política de uso de la tierra, la certificación forestal y la mejora de las capacidades. 28 B. Chan buttresses like Dipterocarps (Primack and Corlett 2005). Timbers from the Dipterocarps dominate the Southeast Asian timber industry as it produces long, straight and knot free logs which are ideal for plywood and sawntimber. This in turn contributed, together with robust economic developments, to a large extent the strong development of the timber industry in Southeast Asia as compared to Africa and Latin America (see Table 1). The denser stocking of the Southeast Asia forests also make them targets for unscrupulous developers of agricultural plantations, mines and hydropower. Clear felling of forests in such 'developments' produces substantial volume of timber for immediate sales in many cases of 'cut and run'. There are, of course, developers who clear the forests and undertake what they had planned to do in line with conditions laid out in the concession: planting crops, building dams, mining or some other economic activity. Forest concessions for logging natural forests are more widely known than other types of concessions like rubber, coffee, cocoa and other agricultural crops/plantations.},
   author = {B Chan},
   issue = {S2},
   journal = {International Forestry Review},
   keywords = {Southeast Asia,capacity building,conflicts,forest concessions,governance},
   pages = {27},
   title = {Southeast Asian Forest Concessions: small steps forward},
   volume = {19},
   year = {2017}
}
